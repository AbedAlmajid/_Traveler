@model DemoTraveler.Models.Partner

@{
    ViewData["Title"] = "Edit";
    Layout = "~/Areas/Administrator/Views/Shared/_AdminLayout.cshtml";
}

<h1>Edit</h1>

<h4>Partner</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="PartnerId" />
            <div class="form-group">
                <label asp-for="PartnerName" class="control-label"></label>
                <input asp-for="PartnerName" class="form-control" />
                <span asp-validation-for="PartnerName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PartnerJop" class="control-label"></label>
                <input asp-for="PartnerJop" class="form-control" />
                <span asp-validation-for="PartnerJop" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PartnerImg" class="control-label"></label>
                <input asp-for="PartnerImg" type="file" class="form-control" />
                <span asp-validation-for="PartnerImg" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

