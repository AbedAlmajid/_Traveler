@model IEnumerable<DemoTraveler.Models.UserTicket>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Administrator/Views/Shared/_AdminLayout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ApplicationUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ticket)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ApplicationUser.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ticket.ArriveTime)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.UserTicketId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.UserTicketId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.UserTicketId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
